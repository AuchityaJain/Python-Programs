def operation(mylist):
    sm = 0
    mx = float('-inf')
    for i in mylist:
        for j in i:
            if j > mx:
                mx = j
            sm = sm + j
    return mx, sm

my_list = [[3, 5, 6], [7, 8, 44], [33, 1, 99]]
mx, sm = operation(my_list)
print(mx, "is the Maximum value")
print(sm, "is the sum")
